{
  "[blade]": {
    "editor.autoClosingBrackets": "always",
    "editor.defaultFormatter": "onecentlin.laravel-blade"
  },
  "[cjs]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.tabSize": 2,
    "editor.indentSize": 2
  },
  "[css]": {
    "editor.defaultFormatter": "aeschli.vscode-css-formatter",
    "editor.tabSize": 2,
    "editor.indentSize": 2
  },
  "[html]": {
    "editor.suggest.insertMode": "replace",
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": false
    }
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.tabSize": 2,
    "editor.indentSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.tabSize": 2,
    "editor.indentSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.tabSize": 2,
    "editor.indentSize": 2
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.tabSize": 2,
    "editor.indentSize": 2
  },
  "[jsx]": {
    "editor.defaultFormatter": "dsznajder.es7-react-js-snippets",
    "editor.tabSize": 2,
    "editor.indentSize": 2
  },
  "[php]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma",
    "editor.tabSize": 2,
    "editor.indentSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.tabSize": 2,
    "editor.indentSize": 2
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2
  },
  "artisan.serve.defaultHost": "127.0.0.1",
  "atlascode.jira.enabled": false,
  "blade.format.enable": true,
  "diffEditor.codeLens": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.wordWrap": "on",
  "editor.cursorBlinking": "expand",
  "editor.detectIndentation": true,
  "editor.fontFamily": "'JetBrains Mono Regular', 'Fira Code', 'Consolas'",
  "editor.fontLigatures": true,
  "editor.fontSize": 25,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.formatOnType": false,
  "editor.insertSpaces": true,
  "editor.lineHeight": 50,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.tabCompletion": "on",
  "editor.tabSize": 4,
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "blade": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 1,
  "files.associations": {
    "*.blade.php": "blade",
    "*.php": "php"
  },
  "files.autoSave": "onFocusChange",
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.ignoreRebaseWarning": true,
  "git.openRepositoryInParentFolders": "always",
  "html.format.enable": true,
  "html.format.templating": true,
  "intelephense.files.exclude": [
    "**/.git/**",
    "**/.svn/**",
    "**/.hg/**",
    "**/CVS/**",
    "**/.DS_Store/**",
    "**/node_modules/**",
    "**/bower_components/**",
    "**/vendor/**/{Tests,tests}/**",
    "**/.history/**",
    "**/vendor/**/vendor/**",
    "**/views/**"
  ],
  "javascript.format.enable": true,
  "jest.autoRun": "off",
  "liveServer.settings.donotShowInfoMsg": true,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "php.files.exclude": [
    "vendor/**/*"
  ],
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "php.validate.executablePath": "D:xampp/php/php.exe",
  "prettier.useTabs": false,
  "redhat.telemetry.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontFamily": "Jetbrains Mono Regular",
  "terminal.integrated.fontSize": 20,
  "vite.autoStart": false,
  "window.commandCenter": true,
  "window.menuBarVisibility": "compact",
  "window.newWindowDimensions": "maximized",
  "window.restoreFullscreen": true,
  "window.zoomLevel": -2.25,
  "workbench.editorAssociations": {
    "*.json": "default"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "yaml.schemas": {
    "file:///c%3A/Users/Gogobli/.vscode/extensions/atlassian.atlascode-3.0.9/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  }
}